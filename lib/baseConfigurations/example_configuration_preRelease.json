{
  "infile": "",
  "firstRelease": false,
  "noVerify": true,
  "silent": false,
  "tagPrefix": "",
  "skip": {"tag": true},
  "commitAll": true,
  "scripts": {"postchangelog": "bin/cleanupChangelog.js"},
  "types": [
    {"type": "feat", "section": "To release"},
    {"type": "fix", "section": "To release"},
    {"type": "chore", "hidden": true},
    {"type": "docs", "hidden": true},
    {"type": "style", "hidden": true},
    {"type": "refactor", "hidden": true},
    {"type": "perf", "hidden": true},
    {"type": "test", "hidden": true}
  ],
  "bumpFiles": [ {"filename": "", "updater": "versionupdate.js"} ],
  "packageFiles": [ {"filename": "", "updater": "versionupdate.js"} ],
  "commitUrlFormat": "{{host}}/{{owner}}/{{repository}}/commits/{{hash}}",
  "compareUrlFormat": "From tag '{{previousTag}}' to current"
}