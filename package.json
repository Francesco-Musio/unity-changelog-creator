{
    "name": "unity-changelog-creator",
    "version": "2.0.5",
    "description": "Changelog Creator designed for Unity",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "author": "Francesco Musio",
    "license": "ISC",
    "type": "module",
    "scripts": {
        "build": "rm -rf dist && rollup -c ./rollup.config.mjs",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "eslint"
    },
    "bin": {
        "changelog-creator": "./dist/index.js"
    },
    "repository": "Francesco-Musio/unity-changelog-creator ",
    "bugs": {
        "url": "https://github.com/Francesco-Musio/unity-changelog-creator/issues"
    },
    "homepage": "https://github.com/Francesco-Musio/unity-changelog-creator#readme",
    "keywords": [
        "conventional-changelog",
        "commit-and-tag-version",
        "recommended",
